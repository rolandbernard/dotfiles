{
    "telemetry.telemetryLevel": "off",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "editor.suggestSelection": "first",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "ltex.additionalRules.enablePickyRules": true,
    "ltex.additionalRules.motherTongue": "de",
    "workbench.startupEditor": "none",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.errorSquiggles": "Disabled",
    "editor.fontFamily": "'IBM Plex Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Comment",
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "comment.line.double-slash",
                    "punctuation.definition.comment",
                ],
                "settings": {
                    "fontStyle": "italic",
                }
            },
        ],
    },
    "editor.renderFinalNewline": false,
    "files.insertFinalNewline": true,
    "editor.lineNumbers": "relative",
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.external.linuxExec": "kitty",
    "editor.renderWhitespace": "all",
    "git.confirmSync": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "explorer.confirmDelete": false,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "clangd.arguments": [
        "-header-insertion=never"
    ],
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-19",
            "path": "/usr/lib/jvm/java",
            "default": true
        },
    ],
    "explorer.sortOrderLexicographicOptions": "lower",
    "latex-workshop.view.pdf.viewer": "tab",
    "ltex.completionEnabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "vscode-neovim.neovimExecutablePaths.win32": "C:\\Users\\Roland\\Documents\\Neovim\\bin\\nvim.exe",
    "security.workspace.trust.untrustedFiles": "open",
    "git.enableSmartCommit": true,
    "jupyter.askForKernelRestart": false,
    "editor.inlayHints.enabled": false,
    "rust-analyzer.inlayHints.enable": false,
    "editor.language.colorizedBracketPairs": [],
    "C_Cpp.clang_format_fallbackStyle": "file",
    "hexeditor.columnWidth": 32,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "javascript.format.enable": false,
    "terminal.integrated.shellIntegration.decorationsEnabled": "overviewRuler",
    "debug.allowBreakpointsEverywhere": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.foldingMaximumRegions": 50000,
    "settingsSync.ignoredExtensions": [
        "llvm-vs-code-extensions.vscode-clangd"
    ],
    "cSpell.userWords": [
        "simplydark"
    ],
    "terminal.integrated.inheritEnv": false,
    "python.analysis.typeCheckingMode": "basic",
    "workbench.colorTheme": "SimplyDark",
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
}
