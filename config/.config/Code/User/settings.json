{
    "telemetry.telemetryLevel": "off",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "editor.suggestSelection": "first",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "ltex.additionalRules.enablePickyRules": true,
    "workbench.startupEditor": "none",
    "C_Cpp.intelliSenseEngine": "disabled",
    "C_Cpp.autocomplete": "disabled",
    "C_Cpp.errorSquiggles": "disabled",
    "editor.fontFamily": "'IBM Plex Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Comment",
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "comment.line.double-slash",
                    "punctuation.definition.comment",
                ],
                "settings": {
                    "fontStyle": "italic",
                }
            },
        ],
    },
    "window.titleBarStyle": "custom",
    "window.dialogStyle": "custom",
    "editor.renderFinalNewline": "off",
    "files.insertFinalNewline": true,
    "editor.lineNumbers": "relative",
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.external.linuxExec": "kitty",
    "editor.renderWhitespace": "all",
    "git.confirmSync": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "explorer.confirmDelete": false,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "clangd.arguments": [
        "-header-insertion=never"
    ],
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-17",
            "path": "/usr/lib/jvm/java-17"
        },
        {
            "name": "JavaSE-20",
            "path": "/usr/lib/jvm/java-20",
            "default": true
        }
    ],
    "explorer.sortOrderLexicographicOptions": "lower",
    "latex-workshop.view.pdf.viewer": "tab",
    "ltex.completionEnabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "vscode-neovim.neovimExecutablePaths.win32": "C:\\Users\\Roland\\Documents\\Neovim\\bin\\nvim.exe",
    "security.workspace.trust.untrustedFiles": "open",
    "git.enableSmartCommit": true,
    "jupyter.askForKernelRestart": false,
    "editor.inlayHints.enabled": "off",
    "rust-analyzer.inlayHints.enable": false,
    "editor.language.colorizedBracketPairs": [],
    "C_Cpp.clang_format_fallbackStyle": "file",
    "hexeditor.columnWidth": 32,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "javascript.format.enable": false,
    "terminal.integrated.shellIntegration.decorationsEnabled": "overviewRuler",
    "debug.allowBreakpointsEverywhere": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.foldingMaximumRegions": 50000,
    "settingsSync.ignoredExtensions": [
        "llvm-vs-code-extensions.vscode-clangd"
    ],
    "terminal.integrated.inheritEnv": false,
    "python.analysis.typeCheckingMode": "basic",
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.showRulerCoverage": true,
    "java.debug.settings.showStaticVariables": false,
    "java.debug.settings.showQualifiedNames": false,
    "java.debug.settings.showLogicalStructure": true,
    "debug.autoExpandLazyVariables": "on",
    "java.format.settings.profile": "${worspaceRoot}/formatter-config.xml",
    "ltex.language": "en-GB",
    "java.compile.nullAnalysis.nonnull": [
        "javax.annotation.Nonnull",
        "org.eclipse.jdt.annotation.NonNull",
        "org.springframework.lang.NonNull"
    ],
    "editor.minimap.renderCharacters": false,
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "workbench.editor.enablePreview": false,
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "xelatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "rnw2tex",
            "command": "Rscript",
            "args": [
                "-e",
                "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
            ],
            "env": {}
        },
        {
            "name": "jnw2tex",
            "command": "julia",
            "args": [
                "-e",
                "using Weave; weave(\"%DOC_EXT%\", doctype=\"tex\")"
            ],
            "env": {}
        },
        {
            "name": "jnw2texminted",
            "command": "julia",
            "args": [
                "-e",
                "using Weave; weave(\"%DOC_EXT%\", doctype=\"texminted\")"
            ],
            "env": {}
        },
        {
            "name": "pnw2tex",
            "command": "pweave",
            "args": [
                "-f",
                "tex",
                "%DOC_EXT%"
            ],
            "env": {}
        },
        {
            "name": "pnw2texminted",
            "command": "pweave",
            "args": [
                "-f",
                "texminted",
                "%DOC_EXT%"
            ],
            "env": {}
        },
        {
            "name": "tectonic",
            "command": "tectonic",
            "args": [
                "--synctex",
                "--keep-logs",
                "%DOC%.tex"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "latexmk (latexmkrc)",
            "tools": [
                "latexmk_rconly"
            ]
        },
        {
            "name": "latexmk (lualatex)",
            "tools": [
                "lualatexmk"
            ]
        },
        {
            "name": "latexmk (xelatex)",
            "tools": [
                "xelatexmk"
            ]
        },
        {
            "name": "pdflatex -> bibtex -> pdflatex * 2",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "Compile Rnw files",
            "tools": [
                "rnw2tex",
                "latexmk"
            ]
        },
        {
            "name": "Compile Jnw files",
            "tools": [
                "jnw2tex",
                "latexmk"
            ]
        },
        {
            "name": "Compile Pnw files",
            "tools": [
                "pnw2tex",
                "latexmk"
            ]
        },
        {
            "name": "tectonic",
            "tools": [
                "tectonic"
            ]
        }
    ],
    "latex-workshop.latex.recipe.default": "latexmk (lualatex)",
    "lldb.suppressUpdateNotifications": true,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.rulers": [
        80, 100, 120
    ],
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#f0f0f010"
    },
    "files.autoSave": "afterDelay",
    "coverage-gutters.coverageFileNames": [
        "coverage/lcov.info",
        "lcov.info",
        "cov.xml",
        "coverage.xml",
        "jacoco.xml",
        "coverage.cobertura.xml"
    ],
    "gitlens.telemetry.enabled": false,
    "rust-analyzer.check.command": "clippy",
    "cmake.showOptionsMovedNotification": false,
    "workbench.colorTheme": "Simple Dark",
    "rust-analyzer.cargo.features": "all",
    "rust-analyzer.cargo.cfgs": {
        "miri": "",
    },
    "[latex]": {
        "editor.wordWrap": "wordWrapColumn",
    },
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
    },
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "[c]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "cSpell.userWords": [
        "perfoming"
    ],
}
